{
  "scripts": {
    "dev": "vite",
    "build": "npx vite build",
    "preview": "vite preview --port 3000",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "backend:install": "cd backend && poetry install",
    "backend:run": "cd backend && poetry run uvicorn src.main:app --reload",
    "backend:migrate": "cd backend && poetry run alembic upgrade head",
    "backend:test": "cd backend && poetry run pytest",
    "e2e": "node scripts/test-runner.js \"npx playwright test --workers=1\"",
    "e2e:ui": "npx playwright test --ui",
    "e2e:headed": "npx playwright test --headed",
    "e2e:install": "npx playwright install",
    "e2e:visual": "node scripts/test-runner.js \"npx playwright test --grep visual-regression --workers=1\"",
    "e2e:visual:update": "npx playwright test --grep visual-regression --update-snapshots",
    "e2e:visual:ci": "npx playwright test --grep visual-regression --shard=1/3",
    "e2e:accessibility": "node scripts/test-runner.js \"npx playwright test --grep accessibility --workers=1\"",
    "e2e:accessibility:ci": "npx playwright test --grep accessibility --shard=1/3",
    "e2e:accessibility:report": "npx playwright show-report",
    "e2e:api": "node scripts/test-runner.js \"npx playwright test --grep api-e2e --workers=1\"",
    "e2e:api:ci": "npx playwright test --grep api-e2e --shard=1/3",
    "e2e:ui-only": "node scripts/test-runner.js \"npx playwright test --grep-not api-e2e --workers=1\"",
    "e2e:parallel": "npm run e2e:api && npm run e2e:ui-only",
    "accessibility:audit:report": "node scripts/accessibility-audit.js --report",
    "accessibility:audit": "node scripts/accessibility-audit.js",
    "performance": "node scripts/test-runner.js \"npx playwright test --grep performance --workers=1\"",
    "performance:ci": "npx playwright test --grep performance --shard=1/2",
    "performance:report": "node scripts/generate-performance-report.js",
    "performance:summary": "node scripts/generate-performance-summary.js",
    "performance:baseline": "node scripts/set-performance-baseline.js",
    "performance:trend": "node scripts/generate-performance-trends.js",
    "test:performance": "npm run performance",
    "test:performance:ci": "npm run performance:ci && npm run performance:report",
    "ci:performance": "npm run performance:ci && npm run performance:summary",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:ci": "vitest run --config vitest.integration.config.ts --coverage",
    "test:aggregate": "node scripts/test-result-aggregator.js",
    "test:dashboard": "node scripts/generate-test-dashboard.js",
    "test:error-scenarios": "vitest run --config vitest.integration.config.ts tests/integration/error-scenarios.test.tsx",
    "test:chaos": "npx playwright test tests/chaos-engineering-e2e.test.ts",
    "test:chaos:ci": "npx playwright test tests/chaos-engineering-e2e.test.ts --shard=1/2",
    "test:error-scenarios:full": "npm run test:error-scenarios && npm run test:chaos",
    "test:error-scenarios:ci": "npm run test:error-scenarios && npm run test:chaos:ci",
    "error-scenario:report": "node scripts/generate-error-scenario-report.js",
    "error-scenario:report:ci": "npm run test:error-scenarios:ci && npm run error-scenario:report",
    "validate:test-setup": "node scripts/validate-test-setup.js",
    "monitor:alerts": "node scripts/test-alert-monitor.js",
    "monitor:check": "npm run monitor:alerts",
    "dashboard:export": "node scripts/dashboard-metrics-exporter.js",
    "ci:test:full": "npm run validate:test-setup && npm run test:integration:ci && npm run e2e:ci && npm run test:aggregate && npm run performance:trend && npm run monitor:check && npm run dashboard:export",
    "ci:test:unit": "npm run validate:test-setup && npm run test:run && npm run test:coverage && npm run dashboard:export",
    "ci:test:e2e": "npm run validate:test-setup && npm run e2e:ci && npm run test:aggregate && npm run dashboard:export",
    "ci:test:performance": "npm run validate:test-setup && npm run performance:ci && npm run performance:trend && npm run dashboard:export",
    "ci:report": "npm run test:aggregate && npm run performance:trend && npm run monitor:check && npm run dashboard:export",
    "ci:artifacts": "mkdir -p test-artifacts && cp -r test-results/* coverage/* playwright-report/* performance-reports/* dashboard-metrics/* test-artifacts/ 2>/dev/null || true",
    "cache:status": "node scripts/cache-manager.ts status",
    "cache:clear": "node scripts/cache-manager.ts clear",
    "cache:invalidate": "node scripts/cache-manager.ts invalidate",
    "build:cached": "npm run cache:status && npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@supabase/supabase-js": "^2.74.0",
    "@tanstack/react-table": "8.17.3",
    "@vercel/analytics": "^1.5.0",
    "axios": "1.6.8",
    "date-fns": "3.6.0",
    "framer-motion": "11.2.10",
    "lucide-react": "0.372.0",
    "next": "^15.5.4",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.4",
    "react-hot-toast": "2.4.1",
    "react-markdown": "9.0.1",
    "react-resizable-panels": "2.0.19",
    "react-router-dom": "^7.9.4",
    "react-simple-code-editor": "0.13.1",
    "recharts": "2.12.7",
    "remark-gfm": "4.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "axe-playwright": "^2.2.2",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  }
}
